<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE sqlMapConfig      
    PUBLIC "-//ibatis.apache.org//DTD SQL Map Config 2.0//EN" 
    "http://ibatis.apache.org/dtd/sql-map-config-2.dtd">
<sqlMapConfig>
    <!-- 系统环境设置 -->
    <settings useStatementNamespaces="true"/>
    
	<typeAlias type="com.sgfm.datacenter.entity.TAutoStrategy"     alias="autoStrategy"/>
	<typeAlias type="com.sgfm.datacenter.entity.TPlaydayStrategy"  alias="playdayStrategy"/>
	<typeAlias type="com.sgfm.datacenter.entity.THolidayStrategy"  alias="holidayStrategy"/>
	<typeAlias type="com.sgfm.datacenter.entity.TSigndayStrategy"  alias="signdayStrategy"/>
	<typeAlias type="com.sgfm.datacenter.entity.TEmployeeOrder"    alias="temployeeOrder"/>
	<typeAlias type="com.sgfm.datacenter.entity.SOrder"  		   alias="sorder"/>
	<typeAlias type="com.sgfm.datacenter.entity.TMember"  		   alias="member"/>
	
    <!-- 全局别名声明 -->	
	<!-- 映射文件注册 (BaseMap.xml必须前置 )-->	
	<sqlMap resource="com/sgfm/base/sqlmap/BaseMap.xml" />
	<sqlMap resource="com/sgfm/datacenter/sqlmap/EmployeeMap.xml" />
	<sqlMap resource="com/sgfm/datacenter/sqlmap/TwechatMap.xml" />
	<sqlMap resource="com/sgfm/datacenter/sqlmap/CalMap.xml" />
	<sqlMap resource="com/sgfm/datacenter/sqlmap/OrderMap.xml" />
	<sqlMap resource="com/sgfm/datacenter/sqlmap/CommonMap.xml" />
	<sqlMap resource="com/sgfm/datacenter/sqlmap/PCUserMap.xml" />
	
   </sqlMapConfig>
